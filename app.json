{
  "expo": {
    "name": "itapastur-mobile",
    "slug": "itapastur-mobile",
    "scheme": "itapastur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo acessa sua galeria para utilizar as fotos do seu empreendimento.",
          "cameraPermission": "O aplicativo acessa sua c√¢mera para registrar as fotos do seu empreendimento."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.itapastur.mobile"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "87bb0027-3147-4fe3-893c-13707168281f"
      }
    },
    "errorRecovery": false
  }
}
